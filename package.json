{
  "name": "@ckir.io/visions",
  "description": "ckir.io/visions is an AI-powered image analysis microservice, originating from the ckir project collection.",
  "version": "1.0.6",
  "license": "MIT",
  "author": {
    "email": "eugen.hildt@gmail.com",
    "name": "Eugen Hildt"
  },
  "bugs": {
    "email": "eugen.hildt@gmail.com"
  },
  "scripts": {
    "build": "nest build --path ./tsconfig.build.json",
    "build:prod": "nest build --path ./tsconfig.build.prod.json",
    "depcheck": "npx depcheck .",
    "depcruise": "npx dependency-cruiser -c .depcruise.mjs --ts-config tsconfig.json src",
    "format": "prettier --write .",
    "lint": "eslint ./src",
    "lint-staged": "npx lint-staged --allow-empty",
    "ncu:update": "npx npm-check-updates -u --format group",
    "ncu:interactive": "npx npm-check-updates -u --interactive --format group",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:debug": "nest start --debug --watch  --preserveWatchOutput",
    "start:dev": "dotenv -e ./.env -- nest start --watch  --preserveWatchOutput",
    "start:node": "node dist/main",
    "test": "jest --passWithNoTests",
    "test:clearCache": "jest --clearCache",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e ./env/.env.e2e -- jest --detectOpenHandles --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "lint:unused": "npx ts-unused-exports ./tsconfig.exclude.json --maxIssues=0"
  },
  "dependencies": {
    "@ehildt/ckir-bullmq": "^0.1.17",
    "@ehildt/ckir-bullmq-logger": "^0.1.14",
    "@ehildt/ckir-config-factory": "^0.1.9",
    "@ehildt/ckir-helpers": "^0.1.22",
    "@ehildt/ckir-ollama": "^0.1.12",
    "@ehildt/ckir-qdrant": "^0.1.8",
    "@ehildt/ckir-socket-io": "^0.1.17",
    "@fastify/compress": "^8.3.1",
    "@fastify/multipart": "^9.4.0",
    "@fastify/static": "^8.3.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.13",
    "@nestjs/core": "^11.1.13",
    "@nestjs/microservices": "^11.1.13",
    "@nestjs/platform-fastify": "^11.1.13",
    "@nestjs/swagger": "^11.2.6",
    "@qdrant/js-client-rest": "^1.16.2",
    "bullmq": "^5.67.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "date-fns": "^4.1.0",
    "fastify": "^5.7.4",
    "ioredis": "^5.9.2",
    "joi": "^18.0.2",
    "ollama": "^0.6.3",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.11",
    "@types/supertest": "^6.0.3",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^17.3.7",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.6",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-extended": "^6.0.0",
    "jest-junit": "^16.0.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^19.3.2",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-unused-exports": "^11.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
